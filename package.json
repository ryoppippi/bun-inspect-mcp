{
  "name": "bun-inspect-mcp",
  "version": "0.1.3",
  "description": "MCP server providing debugging and inspection tools for Bun applications through the Bun Inspector Protocol",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "bun-inspect-mcp": "index.ts"
  },
  "files": [
    "index.ts",
    "browser-types.ts",
    "browser-control.ts",
    "browser-control.js",
    "preview.ts",
    "types.ts",
    "README.md"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "dev": "bun index.ts",
    "typecheck": "tsc --noEmit",
    "build:browser": "bun build ./browser-control.ts --outfile=browser-control.js --target=browser --format=iife",
    "typecheck:browser": "tsc --noEmit -p tsconfig.browser.json"
  },
  "keywords": [
    "bun",
    "inspector",
    "debugger",
    "debug",
    "devtools",
    "dev-tools",
    "protocol",
    "mcp",
    "model-context-protocol",
    "claude",
    "ai-tools",
    "debugging-tools"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryoppippi/bun-inspect-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/ryoppippi/bun-inspect-mcp/issues"
  },
  "homepage": "https://github.com/ryoppippi/bun-inspect-mcp#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/mcp": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "birpc": "^0.2.19",
    "hono": "^4.8.4",
    "zod": "^3.23.8"
  }
}
